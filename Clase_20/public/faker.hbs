
{{#each products}}
    <tr>
        <td>{{this.code}}</td>
        <td>{{this.name}}</td>
        <td>{{this.price}}</td>
        <td>{{this.stock}}</td>
        <td>{{this.description}}</td>
        <td><img style="max-width: 80px;" src="{{this.photoURL}}" alt=""></td>
    </tr>
{{/each}}



